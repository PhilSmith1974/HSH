@model IEnumerable<HSH.Models.PropertySearchModel>
    @using HSH.Areas.Admin.Models;

@{
    ViewBag.Title = "Index";
}
<h2>Search</h2>

<table class="table table-striped table-condensed">
    <tr class="success">
        <th>
            @Html.DisplayNameFor(model => model.TitleKeyword)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PropertyType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfBedrooms)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.County)
        </th>
        
        
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TitleKeyword)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <th>
                @Html.DisplayFor(modelItem => item.PropertyType)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.NumberOfBedrooms)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.County)
            </th>
        </tr>
    }
</table>
