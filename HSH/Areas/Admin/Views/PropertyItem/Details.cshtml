@model HSH.Areas.Admin.Models.PropertyItemModel
    @using HSH.Areas.Admin.Models

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>Property Item</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayNameFor(model => model.ItemTitle)</dt>
        <dd>@Html.DisplayFor(model => model.ItemTitle)</dd>
        <dt>@Html.DisplayNameFor(model => model.PropertyTitle)</dt>
        <dd>@Html.DisplayFor(model => model.PropertyTitle)</dd>
    </dl>
</div>
<p>
    @Html.Partial("_EditButtonDetailPartial", new EditButtonModel { ItemId = Model.ItemId, PropertyId = Model.PropertyId })
    @Html.Partial("_BackToListButtonPartial")
</p>

